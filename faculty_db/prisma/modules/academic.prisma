model SubjectTaught {
  FacultyID    Int
  Level        String
  SubjectName  String
  CourseCode   String?
  ProgramName  String?
  Note         String?

  Faculty Faculty @relation(fields: [FacultyID], references: [FacultyID])

  @@id([FacultyID, Level, SubjectName])
}

model FacultyQualification {
  QualificationID  Int    @id @default(autoincrement())
  FacultyID        Int
  Degree           String
  Institution      String
  YearOfCompletion DateTime

  Faculty Faculty @relation(fields: [FacultyID], references: [FacultyID])
}
